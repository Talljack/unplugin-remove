{
  "name": "unplugin-remove",
  "type": "module",
  "version": "0.1.7",
  "packageManager": "pnpm@8.15.3",
  "description": "Plugin to remove console.xx and debugger for Vite & Webpack & Esbuild & Rollup",
  "license": "MIT",
  "homepage": "https://github.com/Talljack/unplugin-remove#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Talljack/unplugin-remove.git"
  },
  "bugs": {
    "url": "https://github.com/Talljack/unplugin-remove/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "esbuild",
    "transform",
    "remove debugger",
    "remove console",
    "console",
    "debugger"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.js"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.js"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.js"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint src",
    "play:vite": "npm -C playground/vite run dev",
    "play:webpack": "npm -C playground/webpack run serve",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "magic-string": "^0.30.8",
    "unplugin": "^1.8.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.7.0",
    "@types/node": "^20.11.24",
    "bumpp": "^9.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "webpack": "^5.90.3"
  }
}
