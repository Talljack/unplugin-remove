{
  "name": "unplugin-remove",
  "type": "module",
  "version": "0.1.5",
  "packageManager": "pnpm@8.11.0",
  "description": "Register global imports on demand for Vite and Webpack",
  "license": "MIT",
  "homepage": "https://github.com/Talljack/unplugin-remove#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Talljack/unplugin-remove.git"
  },
  "bugs": {
    "url": "https://github.com/Talljack/unplugin-remove/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.js"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.js"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.js"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "play:vite": "npm -C playground/vite run dev",
    "play:webpack": "npm -C playground/webpack run serve",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.2.1",
    "magic-string": "^0.26.3",
    "unplugin": "^1.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@types/node": "^18.0.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esno": "^0.17.0",
    "fast-glob": "^3.2.11",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.29.4",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.5",
    "webpack": "^5.73.0"
  }
}
